			},
			"KnitUnitTests": {
				"$path": "test/tests/unit"
			}
		},
		"TestService": {
			"$className": "TestService",
			"$properties": {
				"ExecuteWithStudioRun": true
			},
			"TestEZ": {
				"$path": "test/testez/default.project.json"
			},
